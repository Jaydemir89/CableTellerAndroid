<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:context="com.japps.joe.cableteller.MainActivity"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/btnCalc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/numLowDevice"
        android:layout_centerHorizontal="true"
        android:text="@string/button"
        app:layout_constraintRight_toLeftOf="@+id/numHighDevice"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="22dp"
        app:layout_constraintTop_toBottomOf="@+id/numHighDevice" />

    <TextView
        android:id="@+id/txtDevice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/txtDemarc"
        android:layout_alignBottom="@+id/txtDemarc"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_gravity="center_vertical"
        android:text="@string/end_point"
        android:textColor="#000000"
        android:textSize="20sp"
        android:layout_marginStart="65dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="70dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="65dp"
        android:layout_marginRight="70dp" />

    <TextView
        android:id="@+id/txtDemarc"
        android:layout_width="0dp"
        android:layout_height="25dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/txtLow"
        android:layout_toRightOf="@+id/txtLow"
        android:text="@string/start_point"
        android:textColor="#000000"
        android:textSize="20sp"
        android:layout_marginStart="18dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/numLowDemarc"
        android:layout_marginTop="16dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="12dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/txtLow"
        android:layout_marginLeft="18dp" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView7"
        android:layout_alignBottom="@+id/textView7"
        android:layout_toEndOf="@+id/numHighDemarc"
        android:layout_toRightOf="@+id/numHighDemarc"
        android:text="@string/dbmv"
        android:textColor="#000000"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/numHighDemarc"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toRightOf="@+id/numHighDemarc" />

    <TextView
        android:id="@+id/txtLow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="@string/low_band"
        android:textColor="#000000"
        android:textSize="18sp"
        android:layout_marginStart="21dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/numLowDemarc"
        tools:layout_constraintBaseline_creator="1"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="21dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView9"
        android:layout_alignBottom="@+id/textView9"
        android:layout_toEndOf="@+id/numLowDemarc"
        android:layout_toRightOf="@+id/numLowDemarc"
        android:text="@string/dbmv"
        android:textColor="#000000"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/numLowDemarc"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="10dp"
        app:layout_constraintLeft_toLeftOf="@+id/txtDemarc"
        android:layout_marginLeft="50dp" />

    <EditText
        android:id="@+id/numLowDemarc"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignBottom="@+id/txtLow"
        android:layout_alignEnd="@+id/txtDemarc"
        android:layout_alignRight="@+id/txtDemarc"
        android:ems="10"
        android:inputType="numberSigned|numberDecimal"
        app:layout_constraintRight_toLeftOf="@+id/textView8"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/numLowDevice"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/txtDemarc"
        app:layout_constraintTop_toTopOf="@+id/numLowDevice" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/txtLow"
        android:layout_toEndOf="@+id/numHighDevice"
        android:layout_toRightOf="@+id/numHighDevice"
        android:text="@string/dbmv"
        android:textColor="#000000"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginTop="153dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/numHighDevice"
        android:layout_marginLeft="4dp"
        android:layout_marginStart="4dp"/>

    <TextView
        android:id="@+id/txtHigh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/numLowDevice"
        android:layout_alignEnd="@+id/txtLow"
        android:layout_alignRight="@+id/txtLow"
        android:text="@string/hi_band"
        android:textColor="#000000"
        android:textSize="18sp"
        android:layout_marginStart="19dp"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/numHighDemarc"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="7dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="19dp" />

    <EditText
        android:id="@+id/numHighDemarc"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_alignLeft="@+id/numLowDemarc"
        android:layout_alignStart="@+id/numLowDemarc"
        android:layout_below="@+id/numLowDemarc"
        android:ems="10"
        android:inputType="numberSigned|numberDecimal"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/numLowDemarc"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@+id/numLowDemarc"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/numLowDemarc" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/txtHigh"
        android:layout_toEndOf="@+id/numLowDevice"
        android:layout_toRightOf="@+id/numLowDevice"
        android:text="@string/dbmv"
        android:textColor="#000000"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/numLowDevice"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="9dp"
        app:layout_constraintLeft_toLeftOf="@+id/textView9" />

    <EditText
        android:id="@+id/numLowDevice"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/numHighDemarc"
        android:layout_alignBottom="@+id/numHighDemarc"
        android:layout_alignLeft="@+id/txtDevice"
        android:layout_alignStart="@+id/txtDevice"
        android:layout_marginBottom="50dp"
        android:layout_marginTop="53dp"
        android:ems="10"
        android:inputType="numberSigned|numberDecimal"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintLeft_toLeftOf="@+id/txtDevice"
        app:layout_constraintRight_toLeftOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/numHighDevice"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_alignBaseline="@+id/numLowDemarc"
        android:layout_alignBottom="@+id/numLowDemarc"
        android:layout_alignLeft="@+id/txtDevice"
        android:layout_alignStart="@+id/txtDevice"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:inputType="numberSigned|numberDecimal"
        app:layout_constraintLeft_toLeftOf="@+id/numLowDevice"
        app:layout_constraintTop_toBottomOf="@+id/numLowDevice"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ScrollView
        android:id="@+id/scrollView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/btnCalc"
        android:fadeScrollbars="false"
        android:isScrollContainer="false"
        android:scrollbarStyle="insideOverlay"
        android:layout_marginEnd="8dp"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/btnCalc"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="8dp">

        <LinearLayout
            android:id="@+id/scrollView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginRight="20dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtResult"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textAlignment="center"
                android:textColor="#000000"
                android:textSize="24sp" />
        </LinearLayout>
    </ScrollView>

</android.support.constraint.ConstraintLayout>
